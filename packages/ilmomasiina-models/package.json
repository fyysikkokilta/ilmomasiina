{
  "name": "@tietokilta/ilmomasiina-models",
  "version": "2.0.0-alpha44",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tietokilta/ilmomasiina.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "node": "./dist/cjs/index.js"
    },
    "./dist/*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "node": "./dist/cjs/*.js"
    },
    "./src/*": "./src/*.ts"
  },
  "scripts": {
    "build": "tsc --build && tsc --build tsconfig.cjs.json",
    "clean": "rimraf dist",
    "typecheck": "tsc --build"
  },
  "devDependencies": {
    "rimraf": "^5.0.10",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.32.35"
  }
}
